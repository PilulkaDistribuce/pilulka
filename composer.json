{
  "name": "pilulka/pilulka",
  "type": "library",
  "require": {
    "php": "^7.1",
    "league/container": "^3.2",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0|^7.5"
  },
  "autoload": {
    "exclude-from-classmap": [
      "packages/container/*Test.php"
    ],
    "psr-4": {
      "Pilulka\\Container\\": "packages/container/src/"
    }
  },
  "replace": {
    "pilulka/container": "self.version"
  }
}
